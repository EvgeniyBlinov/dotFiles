# pretty print "github.com/k0kubun/pp"
snippet pp
pp.Println(${1:})
endsnippet

snippet ld
logrus.Debug("${1:}")
endsnippet

snippet li
logrus.Info("${1:}")
endsnippet

snippet lw
logrus.Warn("${1:}")
endsnippet

snippet le
logrus.WithError(err).Error("${1:}")
endsnippet

snippet irn "If statement return nil, err"
if err != nil {
	return nil, err
}
endsnippet
